{
    "name": "damess/expressive-session-middleware",
    "description": "Simple session middleware for Zend Expressive",
    "keywords": [
        "expressive-session-middleware",
        "psr-7",
        "zend",
        "expressive"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Messenger",
            "email": "dannym87@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "aura/session": "^2.0",
        "psr/http-message": "^1.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "zendframework/zend-diactoros": "^1.1",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "DaMess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DaMess\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
